  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config ./webpack.config.dev.server.ts",
    "dev:build": "webpack --config-name=dev",
    "dist": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --config-name=distProd",
    "dist:dev": "webpack --config-name=distDev",
    "dist:watch": "webpack --config-name=distDevWatch",
    "npm:auth": "uuip-publish-auth [momentum-ui-private]",
    "npm:publish": "npm run npm:auth && npm run dist && uuip-publish dist momentum-ui-private",
    "npm:publish:external": "npm run dist && npm run create-package && cp -R ./README.md ./publish/ && cd ./publish && json -I -f package.json -e 'this.publishConfig={\"registry\": \"https://registry.npmjs.org\"}' && npm publish",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:ci": "jest --coverage --updateSnapshot --maxWorkers=4",
    "storybook:resource": "npx web-component-analyzer src/*components/**/*.ts --outFile .storybook/custom-elements.json",
    "storybook:dist": "npm run storybook:resource && build-storybook -c .storybook -o .out",
    "storybook:local": "npm run storybook:resource && storybook-to-ghpages --dry-run --out=.out && npx http-server .out",
    "storybook": "npm run storybook:resource && start-storybook -p 1234",
    "deploy-storybook": "storybook-to-ghpages",
    "storybook:deploy": "npm run deploy-storybook",
    "storybook:deploy-ci": "storybook-to-ghpages --ci",
    "snapshot": "npm run storybook:resource && build-storybook && percy-storybook --widths=1280",
    "postinstall": "npm run build:tokens",
    "clearVars": "node ./src/utils/clearVars.js",
    "createTokensModule": "node ./src/utils/createTokenFilesModule.js",
    "build:tokens": "yarn createTokensModule && node ./src/utils/generateScssFromTokens.js && node ./src/utils/generateColorTableData.js",
    "sonar": "yarn install && yarn test && node sonar.config.js",
    "eslint": "eslint --ext .ts,.js ./src --ignore-path .eslintignore",
    "eslint:fix": "eslint --ext .ts,.js ./src --ignore-path .eslintignore --fix",
    "create-package": "node -e 'require(\"./publish-conf\").createPackageJson()'",
    "notify": "node -e 'require(\"./publish-conf\").notify()'"
  },
  "resolutions": {
    "jest": "^26.4.2",
