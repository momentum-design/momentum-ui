{
  "name": "@momentum-ui/web-components",
  "version": "3.1.1",
  "author": "Yana Harris",
  "license": "MIT",
  "repository": "https://github.com/momentum-design/momentum-ui.git",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/esm/*.js",
      "require": "./dist/*.js"
    }
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack serve --config ./webpack.config.dev.server.ts",
    "dev:build": "webpack --config-name=dev",
    "dist": "NODE_OPTIONS='--max-old-space-size=4096' webpack --config-name=distProd && NODE_OPTIONS='--max-old-space-size=4096' webpack --config-name=distEsmProd",
    "dist:dev": "webpack --config-name=distDev && webpack --config-name=distEsmDev",
    "dist:watch": "webpack --config-name=distDevWatch",
    "dist:esm": "webpack --config-name=distEsmProd",
    "dist:esm:dev": "webpack --config-name=distEsmDev",
    "npm:auth": "uuip-publish-auth [momentum-ui-private]",
    "npm:publish": "npm run npm:auth && npm run dist && uuip-publish dist momentum-ui-private",
    "npm:publish:local": "npm run dist && npm run create-package && cp -R ./README.md ./publish/ && cd ./publish && json -I -f package.json -e 'this.publishConfig={\"registry\": \"https://registry.npmjs.org\"}' ",
    "npm:publish:external": "yarn npm:publish:local && cd ./publish && npm publish",
    "npm:publish:beta": "yarn npm:publish:local && cd ./publish && npm publish --tag beta",
    "npm:publish:tag": "node scripts/create-release-tag.js",
    "npm:publish:bot": "yarn npm:publish:external && yarn npm:publish:tag",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:ci": "jest --coverage --updateSnapshot --runInBand",
    "storybook:resource": "npx web-component-analyzer src/*components/**/*.ts --outFile .storybook/custom-elements.json",
    "storybook:dist": "npm run storybook:resource && storybook build -c .storybook -o .out",
    "storybook": "npm run storybook:resource && storybook dev -p 1234",
    "snapshot": "npm run storybook:resource && storybook build && percy snapshot ./storybook-static",
    "postinstall": "npm run build:tokens",
    "clearVars": "node ./src/utils/clearVars.js",
    "createTokensModule": "node ./src/utils/createTokenFilesModule.js",
    "build:tokens": "yarn createTokensModule && node ./src/utils/generateScssFromTokens.js && node ./src/utils/generateColorTableData.js",
    "sonar": "yarn install && yarn test && node sonar.config.js",
    "eslint": "eslint --color -f eslint-formatter-pretty",
    "eslint:fix": "eslint --fix --color -f eslint-formatter-pretty",
    "create-package": "node -e 'require(\"./publish-conf\").createPackageJson()'",
    "notify": "node -e 'require(\"./publish-conf\").notify()'",
    "validateIconMapping": "node ./src/utils/validateIconMapping.js"
  },
  "peerDependencies": {
    "@momentum-ui/core": "19.16.0",
    "@momentum-ui/icons": "8.28.5",
    "@momentum-ui/tokens": "1.7.1",
    "@momentum-ui/utils": "6.2.15",
    "lit": "^3.0.0",
    "mobx": "^6.0.0"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@interactjs/actions": "1.10.3",
    "@interactjs/auto-start": "1.10.3",
    "@interactjs/core": "1.10.3",
    "@interactjs/interact": "1.10.3",
    "@interactjs/modifiers": "1.10.3",
    "@interactjs/utils": "1.10.3",
    "@lit-labs/virtualizer": "2.1.0",
    "@momentum-design/icons": "0.43.0",
    "@popperjs/core": "^2.11.8",
    "country-codes-list": "1.6.8",
    "country-flags-svg": "1.1.4",
    "dompurify": "^3.2.4",
    "highlight.js": "^10.5.0",
    "json": "^11.0.0",
    "libphonenumber-js": "^1.7.57",
    "luxon": "^3.7.2",
    "papaparse": "^5.5.3",
    "sortablejs": "^1.15.6"
  },
  "devDependencies": {
    "@adobe/lit-mobx": "^2.2.2",
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
