    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "cssstats": "^4.0.5",
    "eslint": "^9.23.0",
    "eslint-formatter-pretty": "^6.0.1",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.0.1",
    "fs-path": "^0.0.25",
    "handlebars": "^4.7.8",
