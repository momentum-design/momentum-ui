{
  "name": "@collab-ui/react",
  "version": "6.0.0",
  "description": "Cisco Collab UI framework for ReactJs applications",
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "author": "Cisco Systems, Inc.",
  "license": "MIT",
  "keywords": [
    "Cisco",
    "Webex",
    "Cisco Spark",
    "Spark UI",
    "Collab UI",
    "ReactJS"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/collab-ui/collab-ui-react.git"
  },
  "bugs": {
    "url": "https://github.com/collab-ui/collab-ui-react/issues"
  },
  "scripts": {
    "build:markdown": "babel-node ./tools/markdownToHTML.js",
    "build:data": "babel-node ./tools/docCreate.js",
    "build:examples": "babel-node ./tools/exampleCreate.js",
    "build:lib": "BABEL_ENV=lib babel-node tools/lib/build.js",
    "build:docs": "BABEL_ENV=production babel-node tools/website/build.js",
    "build": "run-s build:data build:examples build:lib build:markdown build:docs open:dist",
    "start": "run-p test:watch open:src lint:watch",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "clean:install": "rimraf node_modules && npm install",
    "clean:update": "rimraf node_modules && npm cache verify && npm install && npm update",
    "clean-dist": "npm run remove-dist && mkdir dist bundles lib",
    "lint": "esw webpack.config.* src config --color",
    "lint:watch": "npm run lint -- --watch",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "open:src": "babel-node config/srcServer.js",
    "open:dist": "babel-node config/distServer.js",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "prepublishOnly": "npm run build:lib",
    "prestart": "run-s start-message build:data build:examples",
    "release": "release",
    "remove-dist": "rimraf ./dist ./bundles ./lib",
    "start-message": "babel-node config/startMessage.js",
    "test": "jest",
    "test:cover": "npm run test -- --coverage ",
    "test:CI": "babel-node config/testCi.js",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "watch:docs": "npm-watch"
  },
  "watch": {
    "build:data": "src/lib/**/*.js",
    "build:examples": "src/lib/**/*.js"
  },
  "main": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "module": "lib/index.js",
  "dependencies": {
    "@collab-ui/core": "^9.0.0",
    "babel-runtime": "^6.26.0",
    "moment": "^2.22.0",
    "object-assign": "4.1.1",
    "react-aria-modal": "^2.11.1"
  },
  "peerDependencies": {
    "react": ">=15.6.2 || >=16.0.0",
    "react-dom": ">=15.6.2 || >=16.0.0"
  },
  "devDependencies": {
    "@collab-ui/release": "^2.0.1",
    "@collab-ui/utils": "^2.0.1",
    "autoprefixer": "^7.2.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "2.23.5",
    "chalk": "^2.3.0",
    "child-process-promise": "^2.2.1",
    "classnames": "2.2.5",
    "connect-history-api-fallback": "^1.4.0",
    "copy-webpack-plugin": "^4.3.1",
    "coveralls": "^3.0.0",
    "css-loader": "0.28.8",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-import-resolver-alias": "^1.0.3",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "7.5.1",
    "eslint-watch": "^3.1.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "fs-extra": "^5.0.0",
    "highlight.js": "^9.12.0",
    "history": "4.7.2",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "json-loader": "0.5.7",
    "lodash": "^4.17.4",
    "mockdate": "2.0.2",
    "moment-timezone": "^0.5.14",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "npm-watch": "^0.3.0",
    "opn-cli": "3.1.0",
    "output-file-sync": "^2.0.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "^2.1.3",
    "prompt": "1.0.0",
    "prop-types": "15.6.0",
    "react": "15.6.2",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "15.6.2",
    "react-hot-loader": "^3.1.1",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "react-test-renderer": "15.6.2",
    "react-transition-group": "^2.3.0",
    "redux": "3.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "2.2.0",
    "replace": "0.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "showdown": "^1.8.5",
    "style-loader": "^0.19.1",
    "svgxuse": "^1.2.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "2.9.2",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-md5-hash": "0.0.6"
  },
  "jest": {
    "moduleNameMapper": {
      "^@collab-ui/react(.*)$": "<rootDir>/src/lib/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(scss|css|less)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "./src/helpers/enzyme_setup"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/docs/"
    ]
  }
}
