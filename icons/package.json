{
  "name": "@collab-ui/icons",
  "version": "3.1.0",
  "description": "The Cisco Collab UI Icons library allows developers to easily incorporate Webex Icons and CSS into any application.",
  "keywords": [
    "font",
    "icon",
    "Cisco",
    "Webex",
    "Cisco Spark",
    "Spark UI",
    "Collab UI"
  ],
  "style": "css/collab-ui-icons.css",
  "main": "scss/collab-ui-icons.scss",
  "scripts": {
    "build": "run-s build:lib build:docs",
    "build:docs": "node tools/docs-build.js",
    "build:lib": "node tools/icons-build.js",
    "clean:install": "rimraf node_modules/ && npm i",
    "clean:update": "rimraf node_modules/ && npm i & npm update",
    "prepublishOnly": "npm run build:lib",
    "release": "release",
    "start": "run-s build:lib start:docs",
    "start:docs": "node tools/docs-start.js",
    "test": "node tools/docs-test.js --env=jsdom",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "templates"
      ],
      "extensions": "hbs",
      "quiet": false
    }
  },
  "author": {
    "name": "Paul Jeter",
    "email": "pajeter@cisco.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/collab-ui/collab-ui-icons"
  },
  "bugs": {
    "url": "https://github.com/collab-ui/collab-ui-icons/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@collab-ui/core": "^9.0.1",
    "@collab-ui/release": "^2.0.1",
    "@collab-ui/react": "^6.0.0",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "~1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lodash": "^4.17.5",
    "multi-glob": "^1.0.1",
    "node-sass": "^4.8.3",
    "node-watch": "^0.5.7",
    "npm-run-all": "^4.1.2",
    "npm-watch": "^0.3.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.3.1",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.3.1",
    "react-ga": "^2.4.1",
    "react-highlight": "^0.10.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.0",
    "style-loader": "0.19.0",
    "svg-sprite": "^1.4.0",
    "svg-sprite-loader": "^3.7.3",
    "svgson": "^2.1.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webfonts-generator": "^0.4.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "^2.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {}
}
