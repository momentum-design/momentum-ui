{
  "name": "@momentum-ui/web-components",
  "version": "2.4.1",
  "author": "Yana Harris",
  "license": "MIT",
  "repository": "https://github.com/momentum-design/momentum-ui.git",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run check && webpack-dev-server --config ./webpack.config.dev.server.ts",
    "dev:build": "npm run check && webpack --config-name=dev",
    "dist": "npm run check && webpack --config-name=distProd",
    "dist:dev": "npm run check && webpack --config-name=distDev",
    "dist:watch": "npm run check && webpack --config-name=distDevWatch",
    "check": "check-peer-dependencies --yarn",
    "npm:auth": "uuip-publish-auth [momentum-ui-private]",
    "npm:publish": "npm run npm:auth && npm run dist && uuip-publish dist momentum-ui-private && cd publish && npm publish",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:ci": "jest --coverage --updateSnapshot --maxWorkers=4",
    "storybook:resource": "npx web-component-analyzer src/*components/**/*.ts --outFile .storybook/custom-elements.json",
    "storybook:dist": "npm run storybook:resource && build-storybook -c .storybook -o .out",
    "storybook:local": "npm run storybook:resource && storybook-to-ghpages --dry-run --out=.out && npx http-server .out",
    "storybook": "npm run storybook:resource && start-storybook -p 1234",
    "deploy-storybook": "storybook-to-ghpages",
    "storybook:deploy": "npm run deploy-storybook",
    "storybook:deploy-ci": "storybook-to-ghpages --ci",
    "snapshot": "npm run storybook:resource && build-storybook && percy-storybook --widths=1280",
    "postinstall": "npm run build:tokens",
    "clearVars": "node ./src/utils/clearVars.js",
    "createTokensModule": "node ./src/utils/createTokenFilesModule.js",
    "build:tokens": "yarn createTokensModule && node ./src/utils/generateScssFromTokens.js && node ./src/utils/generateColorTableData.js"
  },
  "resolutions": {
    "jest": "^26.4.2",
    "jest-config": "^26.4.2",
    "lit-element": "2.3.1",
    "lit-html": "1.2.1"
  },
  "peerDependencies": {
    "@momentum-ui/core": "^19.15.3",
    "@momentum-ui/icons": "^8.20.1",
    "@momentum-ui/tokens": "^1.7.1",
    "@momentum-ui/utils": "^6.2.15",
    "lit-element": "2.3.1",
    "lit-html": "1.2.1"
  },
